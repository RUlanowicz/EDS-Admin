<h1 class="page-header">Users</h1>

<div class="table-responsive">

<table class="table table-striped">
    <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>E-mail</th>
      <th>Phone</th>
      <th>Zip</th>
    </tr>
  </thead>

<tbody>
    {{#each report in model}}
        <tr>
          <td>{{report.user.firstName}}</td>
          <td>{{report.user.lastName}}</td>
          <td>{{report.user.email}}</td>
          <td>{{report.user.phone}}</td>
          <td>{{report.user.zip}}</td>
        </tr>
    {{/each}}
  </tbody>
</table>

</div>
